<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#E8A5A0"
    tools:context=".number1">


    <Button
        android:id="@+id/backbtn"
        style="@style/ButtonStyle"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="4dp"
        android:text="back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toEndOf="@+id/button3" />

    <Button
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="45dp"
        android:foreground="@drawable/three"
        style="@style/ButtonStyle"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/button2"
        android:layout_width="0dp"
        android:layout_height="197dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="91dp"
        android:foreground="@drawable/four"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView17"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        style="@style/ButtonStyle"
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="38dp"
        android:foreground="@drawable/five"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView18"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <TextView
        android:id="@+id/textView6"
        style="@style/TextViewStyle"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="106dp"
        android:text="Three"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        style="@style/TextViewStyle"
        android:layout_marginEnd="93dp"
        android:text="Four"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.443" />

    <TextView
        android:id="@+id/textView18"
        style="@style/TextViewStyle"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="164dp"
        android:text="Five"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button3" />

    <Button
        android:id="@+id/nextbtn"
        style="@style/ButtonStyle"
        android:layout_marginStart="7dp"
        android:foreground="@drawable/next"
        app:layout_constraintBaseline_toBaselineOf="@+id/prevbtn"
        app:layout_constraintStart_toEndOf="@+id/prevbtn" />

    <Button
        android:id="@+id/prevbtn"
        style="@style/ButtonStyle"
        android:layout_marginBottom="7dp"
        android:foreground="@drawable/back"
        app:layout_constraintBottom_toTopOf="@+id/backbtn"
        app:layout_constraintEnd_toStartOf="@+id/backbtn"
        app:layout_constraintStart_toStartOf="@+id/backbtn" />
</androidx.constraintlayout.widget.ConstraintLayout>